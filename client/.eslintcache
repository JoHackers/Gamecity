 Name\"\r\n              autoFocus\r\n              onChange={handleChangefirstName}\r\n              type=\"text\"\r\n              name=\"firstName\"\r\n              value={user.firstName}\r\n            />\r\n\r\n            <TextField\r\n              variant=\"outlined\"\r\n              margin=\"normal\"\r\n              required\r\n              fullWidth\r\n              onChange={handleChangelastName}\r\n              type=\"text\"\r\n              name=\"lastName\"\r\n              value={user.lastName}\r\n              placeholder=\"Last Name\"\r\n            />\r\n\r\n            <TextField\r\n              variant=\"outlined\"\r\n              margin=\"normal\"\r\n              required\r\n              fullWidth\r\n              onChange={handleChangecity}\r\n              type=\"text\"\r\n              name=\"city\"\r\n              value={user.city}\r\n              placeholder=\"City\"\r\n            />\r\n\r\n            <TextField\r\n              variant=\"outlined\"\r\n              margin=\"normal\"\r\n              required\r\n              fullWidth\r\n              onChange={handleChangephoneNo}\r\n              type=\"tel\"\r\n              placeholder=\"07X-XXXX-XXX\"\r\n              maxLength=\"10\"\r\n              name=\"phoneNo\"\r\n              value={user.phoneNo}\r\n            />\r\n\r\n            <TextField\r\n              variant=\"outlined\"\r\n              margin=\"normal\"\r\n              required\r\n              fullWidth\r\n              onChange={handleChangebirthday}\r\n              type=\"date\"\r\n              name=\"birthday\"\r\n              value={user.birthday}\r\n            />\r\n\r\n            <br />\r\n\r\n            <Button\r\n              fullWidth\r\n              variant=\"contained\"\r\n              style={{ color: \"white\", backgroundColor: \"#070d13\" }}\r\n              type=\"submit\"\r\n              value=\"Post\"\r\n              onClick={(e) => handleSubmit(e)}\r\n            >\r\n   